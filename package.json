    "@testing-library/user-event": "^12.8.3",
    "@tippyjs/react": "^4.2.6",
    "@types/lodash": "^4.14.177",
    "@types/redux": "^3.6.0",
    "@windmill/react-ui": "^0.6.0",
    "axios": "^0.24.0",
    "chart.js": "^2.9.3",
    "formik": "^2.2.9",
